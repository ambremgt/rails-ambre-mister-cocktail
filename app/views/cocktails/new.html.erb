<div class="banner-new-cocktail" style="background-image: url(https://images.pexels.com/photos/2479243/pexels-photo-2479243.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260);">
  <h1>Create a new cocktail</h1>
</div>

<div>
  <%= simple_form_for @cocktail do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%= f.submit %>
  <% end %>
<%= link_to "Go back", cocktails_path, class: "btn btn-treehouse"%>
</div>
